<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>
  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="BasicHttpBinding_IUsersService" />
        <binding name="BasicHttpBinding_IPatientsService" />
        <binding name="BasicHttpBinding_IService" />
      </basicHttpBinding>
    </bindings>
    <client>
      <endpoint address="http://localhost:55255/Services/UsersService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUsersService"
        contract="ServiceReference.IUsersService" name="BasicHttpBinding_IUsersService" />
      <endpoint address="http://localhost:55255/Services/PatientsService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IPatientsService"
        contract="ServicePatietn.IPatientsService" name="BasicHttpBinding_IPatientsService" />
      <endpoint address="http://localhost:55255/Services/Service.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IService"
        contract="Service.IService" name="BasicHttpBinding_IService" />
    </client>
  </system.serviceModel>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source = |SQL/CE|" />
  </connectionStrings>
</configuration>