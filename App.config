      <endpoint address="http://localhost:55255/Services/UsersService.svc"
        binding="basicHttpBinding" bindingConfiguration="BasicHttpBinding_IUsersService1"
        contract="UsersService.IUsersService" name="BasicHttpBinding_IUsersService1" />
    </client>
  </system.serviceModel>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source = |SQL/CE|" />
  </connectionStrings>
